{
  "name": "promise-mock-api",
  "version": "1.1.4",
  "description": "Mock APIs using classes based in \"Fetch API\" and \"Axios JS\". Using \"Local Storage\" the data are saved, the \"ApiBase\" provides a GET, POST, PUT and DELETE default methods and easily add your own methods.",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drprado2/promise-mock-api.git"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "scripts": {
    "test": "jest",
    "build": "babel src -d lib"
  },
  "keywords": [
    "mock",
    "mock-api",
    "mock-fetch",
    "mock-axios",
    "mock-promise"
  ],
  "tags": [
    "mock",
    "mock-api",
    "mock-fetch",
    "mock-axios",
    "mock-promise"
  ],
  "author": {
    "name": "Adriano Prado de Oliveira",
    "email": "drprado2@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drprado2/promise-mock-api/issues"
  },
  "homepage": "https://github.com/drprado2/promise-mock-api#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-stage-0": "^6.24.1",
    "jest": "^22.4.3"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1"
  }
}
